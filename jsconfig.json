{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "module": "esnext",
    "target": "ES6",
    "paths": {
      "@components": ["./components/index"],
      "@components/*": ["./components/*"],
      "@constants": ["./constants/index"],
      "@constants/*": ["./constants/*"],
      "@helpers": ["./helpers/index"],
      "@helpers/*": ["./helpers/*"],
      "@models": ["./models/index"],
      "@models/*": ["./models/*"],
      "@pages": ["./pages/index"],
      "@pages/*": ["./pages/*"],
      "@utils": ["./utils/index"],
      "@utils/*": ["./utils/*"],
      "@services": ["./services/index"],
      "@services/*": ["./services/*"]
    },
    "jsx": "react-jsx"
  },
  "include": ["./**/*"]
}
